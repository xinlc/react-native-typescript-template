{
  "name": "react-native-typescript-template",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "react-native start",
    "start:theme": "node scripts/custom-rn-theme && react-native start",
    "restart": "node scripts/custom-rn-theme && node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "start:8082": "node scripts/custom-rn-theme && node node_modules/react-native/local-cli/cli.js start --port 8082",
    "start:packager": "yarn react-native start --config packager/config.js",
    "tsc": "tsc",
    "test": "jest",
    "lint": "eslint .",
    "lint:ts": "eslint . src/**/*.ts src/**/*.tsx",
    "tslint": "tslint --project . src/**/*.ts src/**/*.tsx",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/YourAppName/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'"
  },
  "dependencies": {
    "@ant-design/icons-react-native": "1.0.2",
    "@ant-design/react-native": "^3.1.12",
    "@react-native-community/async-storage": "^1.6.1",
    "axios": "^0.19.0",
    "immutable": "^4.0.0-rc.12",
    "iron-redux": "^0.2.7",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-actionsheet": "^2.4.2",
    "react-native-button": "^2.4.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-image-crop-picker": "^0.25.0",
    "react-native-router-flux": "^4.0.6",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.11.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.16",
    "@types/lodash": "^4.14.136",
    "@types/react": "^16.8.24",
    "@types/react-native": "^0.60.3",
    "@types/react-native-actionsheet": "^2.4.1",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-redux": "^7.1.1",
    "@types/react-test-renderer": "^16.8.3",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-saga": "^0.10.5",
    "babel-jest": "^24.8.0",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.1.0",
    "eslint-config-alloy": "^2.0.5",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.8.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "typescript-eslint-parser": "^22.0.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
